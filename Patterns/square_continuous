#include <iostream>
using namespace std;
int main() {
    int n; 
    cout<<"Enter n: ";
    cin>>n;
    int num =1;
    for(int i = 1; i<=n; i++){
        for (int j = 1; j<=n; j++){
            cout<<num<<" ";
            num++;
        }
     cout<<endl;    
    }

    return 0;
}
// for characters,use char ch ='A' inplace of int num=1 and cout<<ch<<" "; ch++; in place  of num output and increment
